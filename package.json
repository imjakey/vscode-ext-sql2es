{
  "name": "sql2es",
  "displayName": "%displayName%",
  "description": "%description%",
  "version": "0.0.5",
  "publisher": "imjakey",
  "engines": {
    "vscode": "^1.74.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/imjakey/vscode-ext-sql2es"
  },
  "activationEvents": [
		"onStartupFinished"
	],
  "i18n": {
    "bundles": {
      "error.emptySQL": "SQL query is empty. Please select some text in the editor.",
      "error.noAPIKey": "API key is not configured. Please set it in the extension settings.",
      "error.apiRequestFailed": "AI model API request failed with status {0}: {1}",
      "error.noAIResults": "AI model returned no results",
      "error.noActiveEditor": "No active editor found!",
      "error.noTextSelected": "No text selected!",
      "error.conversionFailed": "Conversion failed: {0}",
      "error.noHistoryRecords": "No conversion history records found.",
      "history.view.title": "SQL2Es: View Conversion History",
      "history.clear.title": "SQL2Es: Clear Conversion History",
      "history.item.title": "SQL to {0} Conversion",
      "history.item.details": "Converted on {0}",
      "history.item.type.dsl": "Elasticsearch DSL",
      "history.item.type.curl": "Curl Command",
      "history.clear.confirmation": "Are you sure you want to clear all conversion history records?",
      "history.cleared.message": "Conversion history has been cleared.",
      "history.action.viewSQL": "View SQL",
      "history.action.viewResult": "View Result",
      "history.action.copySQL": "Copy SQL",
      "history.action.copyResult": "Copy Result",
      "history.action.insertSQL": "Insert SQL",
      "history.action.insertResult": "Insert Result"
    }
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "SQL",
    "MySQL",
    "Elasticsearch",
    "ES",
    "Es",
    "DSL",
    "converter",
    "mysql",
    "database",
    "数据库"
  ],
  "icon": "images/icon.png",
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sql2es.convert",
        "title": "%command.convert.title%"
      },
      {
        "command": "sql2es.convertToCurl",
        "title": "%command.convertToCurl.title%"
      },
      {
        "command": "sql2es.viewHistory",
        "title": "%history.view.title%"
      },
      {
        "command": "sql2es.clearHistory",
        "title": "%history.clear.title%"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "sql2es.convert",
          "when": "editorHasSelection",
          "group": "navigation"
        },
        {
          "command": "sql2es.convertToCurl",
          "when": "editorHasSelection",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "properties": {
        "sql2es.apiKey": {
          "type": "string",
          "default": "",
          "markdownDescription": "%config.apiKey.description%"
        },
        "sql2es.apiEndpoint": {
          "type": "string",
          "default": "https://api.openai.com/v1/chat/completions",
          "markdownDescription": "%config.apiEndpoint.description%"
        },
        "sql2es.model": {
          "type": "string",
          "default": "gpt-3.5-turbo",
          "markdownDescription": "%config.model.description%"
        },
        "sql2es.esEndpoint": {
          "type": "string",
          "default": "http://localhost:9200",
          "markdownDescription": "%config.esEndpoint.description%"
        },
        "sql2es.esVersion": {
          "type": "string",
          "default": "7.x",
          "markdownDescription": "%config.esVersion.description%"
        },
        "sql2es.esUsername": {
          "type": "string",
          "default": "",
          "markdownDescription": "%config.esUsername.description%"
        },
        "sql2es.esPassword": {
          "type": "string",
          "default": "",
          "markdownDescription": "%config.esPassword.description%"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/node": "22.17.1",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "typescript": "^4.9.4"
  }
}